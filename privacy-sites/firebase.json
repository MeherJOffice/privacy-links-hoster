{
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  },
  "hosting": {
    "public": "web",
    "ignore": [
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store"
          }
        ]
      },
      {
        "source": "/main.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/checkSlug",
        "function": {
          "functionId": "checkSlug",
          "region": "us-central1"
        }
      },
      {
        "source": "/createPolicySite",
        "function": {
          "functionId": "createPolicySite",
          "region": "us-central1"
        }
      },
      {
        "source": "/previewPolicyHtml",
        "function": {
          "functionId": "previewPolicyHtml",
          "region": "us-central1"
        }
      },
      {
        "source": "/publishAiPolicy",
        "function": {
          "functionId": "publishAiPolicy",
          "region": "us-central1"
        }
      },
      {
        "source": "/previewTemplate",
        "function": {
          "functionId": "previewTemplate",
          "region": "us-central1"
        }
      },
      {
        "source": "/publishTemplate",
        "function": {
          "functionId": "publishTemplate",
          "region": "us-central1"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}